{
  "name": "backend-casa-los-lobos-los-gatos",
  "version": "1.0.0",
  "description": "Backend do projeto desenvolvido para Casa Los Lobos e Los Gatos",
  "main": "src/shared/http/server.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "yarn && tsx watch src/shared/http/server.ts",
    "migrate:latest": "tsx node_modules/knex/bin/cli.js migrate:latest",
    "migrate:rollback": "tsx node_modules/knex/bin/cli.js migrate:rollback",
    "migrate:make": "tsx node_modules/knex/bin/cli.js migrate:make",
    "seed:run": "tsx node_modules/knex/bin/cli.js seed:run",
    "seed:make": "tsx node_modules/knex/bin/cli.js seed:make",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "prepare": "husky",
    "build": "tsc && tsc-alias",
    "start": "node dist/shared/http/server.js",
    "check-types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pereirathiago/projeto-casa-los-lobos-los-gatos"
  },
  "lint-staged": {
    "**/*.{js,ts,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mime": "^4.1.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
